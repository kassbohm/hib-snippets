        "from sympy.physics.units import *\n", 
        "from sympy import *\n", 
        "\n", 
        "wAB, theta, lAB = var(\"wAB, theta, lAB\")\n", 
        "ct, st = cos(theta), sin(theta)\n", 
        "rAB = Matrix([lAB*ct, lAB*st, 0])\n", 
        "WAB = Matrix([0,0,wAB])\n", 
        "\n", 
        "vB = WAB.cross(rAB)\n", 
        "pprint(vB)"
